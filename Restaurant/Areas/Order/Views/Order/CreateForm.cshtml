@model Restaurant.Models.ViewModels.OrderVM
@{
    Layout = null;
}

<form>

   

    <div class="form-group">
        <label for="">First Name</label>
        <input type="text" class="form-control" id="firstName"  placeholder="First name">
    </div>

    <div class="form-group">
        <label for="">Last Name</label>
        <input type="text" class="form-control" id="lastName"  placeholder="First name">
    </div>

    <div class="form-group">
        <label for="">Product name</label> 
        <select name="ProductDw" id="productDw" class="form-control" asp-items="@ViewBag.ProductDw" multiple="multiple">
         <option value="">--select--</option>
        </select>
    </div>

    <div class="form-group">
        <label for="">State</label>
        <input type="text" class="form-control" id="state" placeholder="state">

    </div>

    <div class="form-group">
        <label for="">Date</label>
       
        <input class="form-control"  id="orderDate" type="date" />

    </div>

    <button type="submit" id="newOrderSubmit" class="btn btn-primary">Add order</button>
</form>